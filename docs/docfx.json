{
    "build": {
      "content": [
        {
          "files": [
            "**/*.md",
            "**/*.yml"
          ]
        }
      ],
      "resource": [
        {
          "files": [
            "**/*.png",
            "**/*.jpg",
            "**/*.gif",
            "**/*.pdf",
            "**/*.pptx"
          ]
        }
      ],
      "overwrite": [],
      "externalReference": [],
      "globalMetadata": {
        "_appTitle": "RavenRock RP Documentation",
        "_appFooter": "Portions of this page are (c) LuzFaltex 2018-2019 // Powered by <a href=\"https://github.com/dotnet/docfx\">DocFX</a>",
        "_enableSearch": true,
        "titleSuffix": "RavenRockRP Docs",
        "feedback_system": "GitHub",
        "feedback_github_repo": "RavenRockRP/docs",
        "author": "Foxtrek_64"
        },
      "fileMetadata": {},
      "template": [
        "default",
        "_template/light-dark-theme",
        "_template/last-modified",
        "_template/description-generator"
      ],
    "dest": "_site",
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
    }
  }